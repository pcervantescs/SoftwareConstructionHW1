[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = Start a Git repository, run the following experiments: Create two text files and save them to the host repository. Everyone in the group check out (pull) the two text files. Student A updates one file, while Student B updates the other. Both push their updates. All students pull the updates. Student C updates one file, while student B updates the same file in the same location. Student C pushes. Student B pushes. What happens?
[access "refs/*"]
	owner = group user/pcervantescs
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/pcervantescs
	label-Verified = -1..+1 group user/pcervantescs
	push = group user/pcervantescs
	read = group user/pcervantescs
	rebase = group user/pcervantescs
	removeReviewer = group user/pcervantescs
	submit = group user/pcervantescs
[access "refs/heads/*"]
	create = group user/pcervantescs
	push = +force group user/pcervantescs
	pushMerge = group user/pcervantescs
	read = group user/pcervantescs
[access "refs/tags/*"]
	push = group user/pcervantescs
	push = +force group user/pcervantescs
